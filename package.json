{
  "name": "pro-mern-stack",
  "version": "0.0.1",
  "description": "code from examples in pro mern stack publication",
  "main": "app.js",
  "scripts": {
    "start": "nodemon -w dist dist/server.js",
    "compile-server": "babel server --presets es2015-node6 --out-dir dist --source-maps",
    "watch-server": "babel server --presets es2015-node6 --out-dir dist --source-maps --watch",
    "start-hook": "nodemon -w dist dist/start_hook.js",
    "compile": "webpack",
    "watch": "webpack-dev-server --hot --inline",
    "dev-all": "npm run watch && npm run watch-server && npm run start",
    "dev-all-win": "npm-run-all -p -r watch watch-server start",
    "dev-all-hook": "npm run watch & npm run start-hook",
    "lint": "eslint --ext js,jsx src server webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": "https://github.com/MikeAlexMartinez/pro-mern-stack",
  "author": "Michael Martinez",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "mongodb": "^2.2.25",
    "babel-polyfill": "^6.23.0",
    "source-map-support": "^0.4.14",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "react-router": "^2.7.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
